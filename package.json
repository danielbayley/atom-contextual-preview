{
  "name": "contextual-preview",
  "version": "0.1.0",
  "description": "Automatically delegate which preview package to activate based on the active grammar of the current file.",
  "keywords": [
    "preview",
    "context"
  ],
  "engines": {
    "atom": "*"
  },
  "homepage": "http://atom.io/packages/contextual-preview",
  "repository": {
    "type": "git",
    "url": "https://github.com/danielbayley/atom-contextual-preview.git"
  },
  "devDependencies": {
    "cson": "latest"
  },
  "scripts": {
    "postpublish": "[ ! -f package.cson ] && json2cson --tabs package.json > package.cson"
  },
  "license": "MIT"
}
